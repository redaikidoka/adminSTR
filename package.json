{
  "name": "adminstr",
  "version": "11.0.2021-03-16",
  "mindb": "1.20210312",
  "private": true,
  "scripts": {
    "ng": "ng",
    "pug-it": "pug src",
    "pug-it:w": "pug src -P -w",
    "start": "ng serve --hmr --port 4220",
    "start:pug": "npm run pug-it:w & ng serve --hmr --port 4220",
    "server": "ng serve --port 4220",
    "build": "ng build",
    "build:pug": "npm run pug-it & ng build",
    "test": "ng test",
    "lint": "ng lint",
    "build:demo": "pug src && ng build --configuration=demo",
    "deploy:demo": "gcloud config configurations activate webstr-dev; npm run build:demo; gcloud app deploy -q --version test",
    "deploy:demo:ver": "gcloud config configurations activate webstr-dev; npm run build:demo; gcloud app deploy -q --version 11-15-2021-02-09",
    "build:psi": "pug src && ng build --configuration=production",
    "deploy:psi": "gcloud config configurations activate webstr-psi && npm run build:psi && gcloud app deploy -q --version trial",
    "deploy:psi:ver": "gcloud config configurations activate webstr-psi && npm run build:psi && gcloud app deploy -q --version 11-15-2021-02-09",
    "build:psi-test": "pug src && ng build --configuration=test",
    "deploy:psi-test": "gcloud config configurations activate webstr-psi && npm run build:psi-test && gcloud app deploy -q --version test --no-promote"
  },
  "dependencies": {
    "@angular/animations": "~11.1.1",
