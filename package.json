    "graphql": "^15.0.0",
    "graphql-tag": "^2.11.0",
    "lodash": "^4.17.20",
    "ng-cli-pug-loader": "^0.2.2",
    "rollbar": "^2.21.0",
    "rxjs": "~6.6.0",
    "subsink": "^1.0.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
